{
  "name": "@reshare/rs",
  "version": "1.6.0",
  "description": "Resource Sharing",
  "main": "src/index.js",
  "repository": "",
  "license": "Apache-2.0",
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test-int": "stripes test nightmare --run demo --show",
    "test": "stripes test karma",
    "lint": "eslint src"
  },
  "devDependencies": {
    "postcss": "^7.0.0",
    "postcss-calc": "^7.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-media": "^7.0.0",
    "postcss-custom-properties": "^10.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-nesting": "^7.0.1",
    "postcss-syntax": "^0.34.0",
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes-cli": "~2.1.0",
    "babel-eslint": "^10.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.0.0",
    "mocha": "^5.2.0",
    "sinon": "^6.3.4"
  },
  "dependencies": {
    "@folio/react-intl-safe-html": "^3.0.0",
    "@folio/stripes-erm-components": "^5.0.0",
    "@reshare/stripes-reshare": "^1.6.0",
    "@folio/stripes-template-editor": "^3.0.0",
    "classnames": "^2.2.6",
    "compose-function": "^3.0.3",
    "final-form-arrays": "^3.0.2",
    "graphql-tag": "^2.5.0",
    "handlebars": "^4.5.3",
    "html-to-react": "^1.4.2",
    "jsbarcode": "^3.11.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.4",
    "postcss-nested": "^4.1.2",
    "prop-types": "^15.6.0",
    "query-string": "^6.10.1",
    "react-apollo": "^2.1.3",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.1",
    "react-to-print": "^2.5.0",
    "reset-css": "^5.0.1",
    "rrule": "2.6.4",
    "xmldom": "^0.1.27"
  },
  "peerDependencies": {
    "@folio/stripes": "^6.1.0",
    "moment": "^2.29.0",
    "react": "*",
    "react-intl": "^5.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.0.0",
    "react-query": "^3.9.0"
  },
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-rs.meta.title",
    "route": "/rs",
    "queryResource": "query",
    "stripesDeps": [
      "@folio/stripes-erm-components",
      "@reshare/stripes-reshare",
      "@folio/stripes-template-editor"
    ],
    "icons": [
      {
        "name": "app",
        "alt": "Create, view and manage patron requests",
        "title": "Requests"
      }
    ],
    "okapiInterfaces": {
      "rs": "1.0 2.0",
      "tags": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.rs.enabled",
        "displayName": "UI: resource-sharing module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.rs.enabled",
        "displayName": "Settings (resource-sharing): settings enabled",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-rs.settings.system",
        "displayName": "Settings (resource-sharing): can view and edit system settings that do not have separate permissions",
        "subPermissions": [
          "settings.rs.enabled",
          "rs.settings.get",
          "rs.settings.put",
          "rs.refdata.get",
          "rs.refdata.put"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-rs.settings.notices",
        "displayName": "Settings (resource-sharing): Can create, edit and remove notices and notice policies",
        "subPermissions": [
          "settings.rs.enabled",
          "rs.noticePolicies",
          "rs.refdata.get",
          "rs.template.write"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-rs.settings.hostlmslocations",
        "displayName": "Settings (resource-sharing): Can edit host LMS locations",
        "subPermissions": [
          "settings.rs.enabled",
          "rs.directoryentry.get",
          "rs.hostlmslocations"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-rs.settings.pullslip-notifications",
        "displayName": "Settings (resource-sharing): Can create, edit and remove pullslip notifications and templates",
        "subPermissions": [
          "settings.rs.enabled",
          "rs.hostlmslocations.get",
          "rs.timers",
          "rs.refdata.get",
          "rs.template.write"
        ],
        "visible": true
      }
    ]
  }
}
